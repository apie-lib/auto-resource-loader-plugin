{
  "name": "apie/auto-resource-loader-plugin",
  "description": "Composer package of the apie library: auto resource loader plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\AutoResourceLoaderPlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\AutoResourceLoaderPlugin\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/core": "dev-main",
    "haydenpierce/class-finder": "^0.4.0"
  },
  "require-dev": {
    "apie/mock-objects": "dev-main",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}